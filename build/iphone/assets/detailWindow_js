function detailWindow(e){var t=createActInd(),i=Titanium.UI.createWindow({statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,barImage:"images/header.png",titleControl:Ti.UI.createImageView({image:"images/detail_title_label.png",height:24,width:88})});i.hideTabBar();var a=Titanium.UI.createButton({backgroundImage:"images/back_btn.png",width:50.3,height:32});i.leftNavButton=a,a.addEventListener("click",function(){i.close()});var r=Titanium.UI.createLabel({font:{fontSize:16,fontWeight:"bold"},text:"通報",textAlign:"center",verticalAlign:Titanium.UI.TEXT_VERTICAL_ALIGNMENT_CENTER,borderRadius:4,height:25,width:60,right:0,color:"red"});i.setRightNavButton(r),r.addEventListener("click",function(){t.show();var i=Ti.App.domain+"send_report.json",a={id:e.id,user_id:e.user_id,luck_restriction:e.luck_restriction,desc:e.desc,url:e.url,name:e.name,image:e.image,access:e.access,time:e.time};sendData(i,a,function(e){e.success?(Ti.UI.createAlertDialog({title:"通報を送信しました"}).show(),t.hide()):(Ti.UI.createAlertDialog({title:"通報の送信に失敗しました"}).show(),t.hide())})}),webView=Ti.UI.createWebView({url:"/detail.html",backgroundColor:"black"});var n=function(){var t="getContents(name = '"+e.name+"', desc = '"+e.desc+"', image = '"+e.image+"')";webView.evalJS(t)};Ti.App.addEventListener("webviewload",n);var o=function(){recruitID=e.id,luck_restriction=e.luck_restriction;var t={id:recruitID};url=Ti.App.domain+"increment_access.json",sendData(url,t,function(e){e.success}),Ti.Platform.openURL(e.url)};return Ti.App.addEventListener("app:fromWebView",o),i.addEventListener("close",function(){Ti.App.removeEventListener("app:fromWebView",o),Ti.App.removeEventListener("webviewload",n)}),i.add(webView),i.add(t),i}module.exports=detailWindow;